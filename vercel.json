{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/build/web/$1"
        },
        {
            "source": "/",
            "destination": "/build/web/index.html"
        }
    ],
    "headers": [
        {
            "source": "/build/web/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}